<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE persons [
        <!ELEMENT persons (person)>
        <!ELEMENT person (name,age)>
        <!ELEMENT name (#PCDATA)>
        <!ELEMENT age (#PCDATA)>
        ]>
<!--dtd约束的第二种方式，在xml内部引入dtd约束-->

<persons>
    <person>
        <name>张三</name>
        <age>23</age>
    </person>

</persons>